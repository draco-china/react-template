{
  "editor.defaultFormatter": "biomejs.biome",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.biome": "explicit",
    "source.organizeImports.biome": "explicit"
  },
  "files.watcherExclude": {
    "**/routeTree.gen.ts": true
  },
  "search.exclude": {
    "**/routeTree.gen.ts": true
  },
  "files.readonlyInclude": {
    "**/routeTree.gen.ts": true
  },
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "package.json": "bun.lock,package-lock.json,pnpm-lock.yaml,yarn.lock,npm-shrinkwrap.json,.releaserc.json,commitlint.config.cjs,lefthook.yml,components.json,pwa-assets.config.ts",
    "README.md": "LICENSE,CHANGELOG.md,.env.example,.env,.gitignore",
    "tsconfig.json": "tsconfig.*.json,biome.json,biome.jsonc,vite.config.*,index.html"
  }
}
