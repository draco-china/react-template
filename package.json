{
  "name": "react-template",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.3.0",
  "engines": {
    "bun": ">=1.3.0"
  },
  "scripts": {
    "dev": "vite --port 3000",
    "build": "vite build && tsc",
    "preview": "vite preview",
    "test": "vitest run",
    "tsc": "tsc --noEmit",
    "pwa:assets": "pwa-assets-generator",
    "pwa:html": "bun scripts/update-pwa-html.ts",
    "pwa:prepare": "bun run pwa:assets && bun run pwa:html",
    "openapi:generate": "openapi-typescript $OPENAPI_SCHEMA_URL -o src/lib/api/schema.d.ts",
    "check": "ultracite check",
    "fix": "ultracite fix",
    "prepare": "lefthook install",
    "release": "semantic-release"
  },
  "dependencies": {
    "@base-ui/react": "^1.1.0",
    "@faker-js/faker": "^10.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/match-sorter-utils": "^8.19.4",
    "@tanstack/react-devtools": "^0.9.4",
    "@tanstack/react-form": "^1.28.0",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-query-devtools": "^5.91.3",
    "@tanstack/react-router": "^1.158.0",
    "@tanstack/react-router-devtools": "^1.158.0",
    "@tanstack/react-store": "^0.8.0",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/router-plugin": "^1.158.0",
    "@tanstack/store": "^0.8.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.563.0",
    "next-themes": "^0.4.6",
    "openapi-fetch": "^0.15.0",
    "radix-ui": "^1.4.3",
    "react": "^19.2.4",
    "react-day-picker": "^9.13.0",
    "react-dom": "^19.2.4",
    "react-hook-form": "^7.71.1",
    "react-resizable-panels": "^4",
    "recharts": "2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "vaul": "^1.1.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.14",
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@inlang/paraglide-js": "^2.10.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.3",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@tanstack/devtools-event-client": "^0.4.0",
    "@tanstack/devtools-vite": "^0.5.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^25.2.0",
    "@types/react": "^19.2.11",
    "@types/react-dom": "^19.2.3",
    "@vite-pwa/assets-generator": "^1.0.2",
    "@vitejs/plugin-react": "^5.1.3",
    "jsdom": "^28.0.0",
    "lefthook": "^2.1.0",
    "lint-staged": "^16.2.7",
    "openapi-typescript": "^7.10.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "semantic-release": "^25.0.3",
    "typescript": "^5.9.3",
    "ultracite": "7.1.4",
    "vite": "^8.0.0-beta.12",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.18",
    "web-vitals": "^5.1.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,jsonc,css,scss,md,mdx}": [
      "bun x ultracite fix"
    ]
  }
}
